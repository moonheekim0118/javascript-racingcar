{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": { "import/extensions": ["off"],"no-undef":"off" , 
        "no-unused-vars":"off",
    "class-methods-use-this":"off",
    "consistent-return":"off",
    "array-callback-return":"off",
    
}
}
